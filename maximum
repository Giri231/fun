#include<iostream>

using namespace std;

int
maximum (int n,int a[])
{
  int  i;
  int t = a[0];
  for (i = 1; i < n; i++)
    {
      if (t < a[i])
	{
	  t = a[i];


	}

    }
  return t;


}

int main ()
{
  int n;
  cin >> n;
  int a[n];
  for (int i = 0; i < n; i++)
    {
      cin >> a[i];
    }
  cout << maximum (n,a);
  return 0;
}
