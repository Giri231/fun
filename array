#include<iostream>

using namespace std;

int largest_ele(int a[])
{
    int i;
    int l=a[0];
    for(i=1;i<a;i++)
    {
        if(l<a[i+1])
        {
            l=a[i+1];
        }
    }
    return l;
}

int main()
{
    int n,i;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
   cout<<largest_ele(a);
    return 0;
    
}
