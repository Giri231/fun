#include<iostream>
using namespace std;

int perfect(int a)
{
    int i;
    int sum=0;
    int num=a;
    for(i=1;i<num;i++)
    {
        if(num%i==0)
        
         {
             sum+=i;    
         }
    }
    if(a==sum)
    {
        return 0;
    }
    else
    return 1;
  

}

int main()
{
    int a;
    cin>>a;
   cout<< perfect(a);
   return  0;
    
    
}
